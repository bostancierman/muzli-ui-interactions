{
  "name": "Muzli-UI-Interactions",
  "version": "1.0.0",
  "description": "A series of UI interactions, inspired from Muzli-featured designers, and implemented using modern web technologies.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samageloff/muzli-design-interactions.git"
  },
  "bugs": {
    "url": "https://github.com/samageloff/muzli-design-interactions/issues"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": "4.4.3",
    "npm": "3.10.10"
  },
  "author": "Sam Ageloff",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "npm run clean && export NODE_ENV=production && webpack --config ./webpack.config.js --profile --colors",
    "clean": "rimraf dist lib",
    "postinstall": "npm run build",
    "babel-node": "babel-node"
  },
  "jest": {
    "modulePaths": [
      "/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "axios": "^0.11.0",
    "deepmerge": "^2.0.0",
    "immutable": "^3.8.1",
    "prop-types": "~15.5.10",
    "react-hot-loader": "^3.1.2",
    "react-id-swiper": "^1.5.6",
    "react-immutable-proptypes": "^2.1.0",
    "react-ions": "2.0.20",
    "react-redux": "^5.0.5",
    "react-transition-group": "^2.2.1",
    "redux": "~3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "styled-components": "2.1.2",
    "swiper": "^4.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.31",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-airbnb": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.19.0",
    "dotenv": "^2.0.0",
    "envify": "^3.4.0",
    "enzyme": "~2.8.2",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.4",
    "map-obj": "^2.0.0",
    "node-sass": "~3.4.2",
    "postcss-loader": "^0.6.0",
    "react": "16.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.0.0",
    "react-test-renderer": "^16.1.1",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "static-loader": "^0.1.8",
    "style-loader": "^0.12.4",
    "transform-loader": "^0.2.3",
    "url-loader": "^0.5.7",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
